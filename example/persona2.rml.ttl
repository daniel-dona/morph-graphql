        <43LR95CY57>
        rml:logicalSource [
            rml:source  "Personas";
            rml:referenceFormulation ql:JSONPath;
        ];


        rr:subjectMap [
            rml:reference "id";
            rr:class schema:Person
        ];
        
        
        rr:predicateObjectMap [
          rr:predicate schema:name;
          rr:objectMap    [ rml:reference "nombre" ]
        ];
 
         rr:predicateObjectMap [
          rr:predicate schema:height;
          rr:objectMap    [ rml:reference "altura"; rr:datatype xsd:positiveInteger ]
        ];

    .
    
